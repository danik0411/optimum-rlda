# this code will collect the data from the same alpha, estimator and feature
# size into a signle file, it will easier to use in the future

rm( list = ls(all=TRUE));

repetition = 500;
sampleSize = seq(30,100, by=10);
gammaBase = 100^(1/20);
gammaValues = gammaBase^c(-10:20);

allFileNames = list.files();

# collect double a1, iterate by p

all_double = grep(x=allFileNames, pattern = "double", value=TRUE);

a1_double = grep(x = all_double, pattern = "1a", value=TRUE);

a1_double_5p = grep(x = a1_double, pattern = "5p" , value=TRUE);

print(a1_double_5p)

print("creating variables");
totalErrorDouble_1a_5p = array(0, dim=c(repetition, length(gammaValues),
length(sampleSize)));
totalTimeDouble_1a_5p = array(0, dim=c(length(sampleSize),repetition));

for ( s in 1:length(sampleSize) ){
tempString = grep( x = a1_double_5p,
	     pattern = paste( as.character(sampleSize[s]) , "s", sep=""),
	     value=TRUE);
print(tempString);
tempVarList = load(tempString);
totalErrorDouble_1a_5p[,,s] = totalErrorDouble;
totalTimeDouble_1a_5p[s,] = totalTimeDouble;
rm(tempVarList);
}

print("finished, moving to a1_double_20p");
a1_double_20p = grep(x = a1_double, pattern = "20p", value=TRUE);

print(a1_double_20p);

print("creating variables");
totalErrorDouble_1a_20p = array(0, dim=c(repetition, length(gammaValues),
length(sampleSize)));
totalTimeDouble_1a_20p = array(0, dim=c(length(sampleSize),repetition));

for ( s in 1:length(sampleSize) ){
tempString = grep( x = a1_double_20p,
	     pattern = paste( as.character(sampleSize[s]) , "s", sep=""),
	     value=TRUE);
print(tempString);
tempVarList = load(tempString);
totalErrorDouble_1a_20p[,,s] = totalErrorDouble;
totalTimeDouble_1a_20p[s,] = totalTimeDouble;
rm(tempVarList);
}

print("finished, moving to a1_double_50p");
a1_double_50p = grep(x = a1_double, pattern = "50p", value=TRUE);

print(a1_double_50p);

print("creating variables");
totalErrorDouble_1a_50p = array(0, dim=c(repetition, length(gammaValues),
length(sampleSize)));
totalTimeDouble_1a_50p = array(0, dim=c(length(sampleSize),repetition));

for ( s in 1:length(sampleSize) ){
tempString = grep( x = a1_double_50p,
	     pattern = paste( as.character(sampleSize[s]) , "s", sep=""),
	     value=TRUE);
print(tempString);
tempVarList = load(tempString);
#totalErrorDouble_1a_50p[,,s] = totalErrorDouble;
#totalTimeDouble_1a_50p[s,] = totalTimeDouble;
rm(tempVarList);
}